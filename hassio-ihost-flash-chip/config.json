{
    "name": "Test Dongle Flasher For iHost",
    "version": "1.2.2",
    "slug": "sonoff_dongle_flasher_for_ihost",
    "description": "Flash the built-in MG21 (Zigbee coordinator) firmware on iHost with one-click flashing through the web interface.",
    "uart": true,
    "udev": true,
    "url": "https://github.com/iHost-Open-Source-Project/hassio-ihost-addon/tree/master/hassio-ihost-hardware-control",
    "startup": "application",
    "boot": "auto",
    "arch": ["armv7", "aarch64", "amd64"],
    "init": false,
    "panel_icon": "mdi:memory-arrow-down",
    "panel_title": "Test Dongle Flasher For iHost",
    "ingress": true,
    "ingress_port": 8324,
    "ingress_stream": true,
    "host_network": true,
    "homeassistant_api": true,
    "hassio_role": "admin",
    "hassio_api": true,
    "kernel_modules": true,
    "full_access": true,
    "apparmor": false,
    "gpio": true,
    "privileged": ["SYS_ADMIN", "SYS_MODULE", "SYS_RAWIO", "SYS_RESOURCE"],
    "map": [
        {
            "type": "addon_config",
            "read_only": false,
            "path": "/Sonoff-Dongle-Flasher-For-IHost"
        },
        {
            "type": "homeassistant_config",
            "read_only": false,
            "path": "/config"
        }
    ],
    "options": {
        "data_path": "/config/iHost-Flash-Addon-Test"
    },
    "schema": {
        "data_path": "str"
    },
    "image": "ghcr.io/suikasan/flash-test-addon-{arch}"
}
