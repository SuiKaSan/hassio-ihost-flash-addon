{
    "name": "Flash Test MG21 Addon",
    "version": "0.0.1",
    "slug": "flash-test-mg21-addon",
    "description": "Just testing flash",
    "uart": true,
    "udev": true,
    "url": "https://github.com/iHost-Open-Source-Project/hassio-ihost-addon/tree/master/hassio-ihost-hardware-control",
    "startup": "application",
    "boot": "auto",
    "arch": ["armv7"],
    "init": false,
    "ports": {
        "7321/tcp": 7321
    },
    "ports_description": {
        "7321/tcp": "Web interface"
    },
    "panel_icon": "mdi:earth",
    "panel_title": "eWeLink Smart Home",
    "webui": "http://[HOST]:[PORT:7321]",
    "ingress": true,
    "ingress_port": 7321,
    "ingress_entry": "",
    "homeassistant_api": true,
    "map": [
        "data:rw",
        "config:rw",
        {
            "type": "share",
            "read_only": false
        },
        {
            "type": "homeassistant_config",
            "read_only": false,
            "path": "/config"
        }
    ],
    "options": {
        "data_path": "/config/hass-ihost"
    },
    "schema": {
        "data_path": "str"
    },
    "hassio_role": "manager",
    "hassio_api": true,
    "image": "ghcr.io/suikasan/flash-test-addon-{arch}",
    "devices": ["/dev/ttyS4"]
}
